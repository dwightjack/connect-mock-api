{
  "name": "connect-mock-api",
  "version": "1.2.0",
  "description": "Fake API server and middleware for connect and express",
  "author": "Marco Solazzi",
  "license": "MIT",
  "main": "./index.js",
  "files": [
    "lib",
    "bin",
    "index.js"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "start": "nodemon ./lib/cli.js --watch lib index.js --config=./fixtures/config.js"
  },
  "keywords": [
    "api",
    "connect",
    "express",
    "middleware"
  ],
  "engines": {
    "node": ">=8.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dwightjack/connect-mock-api.git"
  },
  "bugs": {
    "url": "https://github.com/dwightjack/connect-mock-api/issues"
  },
  "homepage": "https://github.com/dwightjack/connect-mock-api",
  "jest": {
    "verbose": true,
    "bail": false,
    "collectCoverageFrom": [
      "**/lib/**/*.js",
      "index.js",
      "!**/__mocks__/**",
      "!**/__tests__/**"
    ],
    "collectCoverage": false,
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.js"
    ]
  },
  "dependencies": {
    "chokidar": "~2.0.3",
    "lodash": "4.17.10",
    "path-to-regexp": "2.2.1",
    "require-uncached": "~1.0.3",
    "yargs": "11.0.0"
  },
  "devDependencies": {
    "chance": "~1.0.16",
    "connect": "~3.5.0",
    "eslint": "~4.19.1",
    "eslint-plugin-node": "~6.0.1",
    "jest": "~23.1.0",
    "koa": "~2.5.1",
    "nodemon": "~1.17.0"
  }
}
